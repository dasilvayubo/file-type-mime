{
  "name": "file-type-mime",
  "version": "0.0.5",
  "description": "Utility to parse mime type from a binary file",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "check": "rome check src",
    "format": "rome format src --write",
    "build": "rm -rf dist && tsc",
    "test": "mocha --require ts-node/register src/*.spec.ts"
  },
  "keywords": [
    "mime",
    "file",
    "type",
    "content",
    "content-type",
    "parse"
  ],
  "author": {
    "name": "Edmundas Ramanauskas",
    "email": "edmundas.ramanauskas@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/redmundas/file-type-mime.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.3.5",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.2.5",
    "chai": "^4.3.7",
    "mocha": "^10.2.0",
    "rome": "12.1.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
}
